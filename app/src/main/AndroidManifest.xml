<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.cmps359_logger_final_project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/poggers_emote"
        android:label="@string/Loggers"
        android:roundIcon="@mipmap/poggers_emote"
        android:supportsRtl="true"
        android:theme="@style/Theme.CMPS359_Logger_Final_Project"
        tools:ignore="AllowBackup">
        <activity
            android:name=".MainActivity"
            android:label="@string/Loggers"
            android:theme="@style/Theme.CMPS359_Logger_Final_Project.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>